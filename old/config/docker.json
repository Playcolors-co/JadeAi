{
    "version": "1.0.0",
    "required_docker_version": "24.0.0",
    "status_file": "/opt/control-panel/.docker_setup",
    "config_file": "/opt/control-panel/config/docker.json",
    "backup_dir": "/opt/control-panel/backups/docker",
    "required_packages": [
        "docker.io",
        "docker-compose"
    ],
    "network": {
        "subnet": "172.29.0.0/16",
        "gateway": "172.29.0.1"
    },
    "volume": {
        "data_path": "/var/lib/docker/volumes"
    },
    "daemon_settings": {
        "max-concurrent-downloads": 3,
        "max-concurrent-uploads": 5,
        "dns": ["8.8.8.8", "8.8.4.4"]
    },
    "daemon_config": {
        "data-root": "/var/lib/docker",
        "log-driver": "json-file",
        "log-opts": {
            "max-size": "10m",
            "max-file": "3"
        },
        "storage-driver": "overlay2",
        "registry-mirrors": [],
        "experimental": false,
        "debug": false
    },
    "daemon_config_path": "/etc/docker/daemon.json",
    "service_config_path": "/etc/systemd/system/docker.service.d/override.conf",
    "compose_version": "2.17.2"
}
